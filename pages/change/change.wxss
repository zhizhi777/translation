
.container {
    background: #f8f8f8;
}
.lang-list {
    display: flex;
    flex-direction: column;
}

.title{
    background: #fff;
    border-bottom: 1px solid #ececec;
    padding: 20rpx 40rpx;
    color: #aaa;
    font-size: 28rpx;
}

.item {
    padding-left: 40rpx;
    background-color: #fff;
  }
.item-inner{
    border-bottom: 1px solid #ececec;
    padding: 20rpx 40rpx 20rpx 0;
    font-size: 28rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
}
.item:last-child .item-inner{
    border-bottom:none;
}
.icon-duihao{
    font-size: 30rpx;
    color:#aaa;
}